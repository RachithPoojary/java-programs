
import java.util.Scanner;

class pattern3 
{
	public static void main(String[] args) 
	{
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter the value of n");
		int n = sc.nextInt();

		for (int i = n;i>=n ;i-- )
		{
			for (int j= i;j>=i ;j-- )
			{
				System.out.print("*");
			}
			System.out.println();
		}
}
}
